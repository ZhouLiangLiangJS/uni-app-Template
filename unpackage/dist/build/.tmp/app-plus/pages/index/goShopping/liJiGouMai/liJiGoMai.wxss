
page{
	background: #F1F1F1;
}
.fr{
	float: right;
}
.tianJia{
	position: absolute;
	-webkit-transform: translate(-50%,-50%);
	    -ms-transform: translate(-50%,-50%);
	        transform: translate(-50%,-50%);
	left: 50%;
	top: 50%;
	font-size: 50rpx;
}
.hader-box{
	width: 100%;
	height: 168rpx;
	background: #F53D4A;
	position: fixed;
	top: 0;
	z-index: 10;
}
.hader-box-back{
	width: 40rpx;
	height: 40rpx;
	margin-top: 75rpx;
	margin-left: 20rpx;
}
.hader-box-text{
	color: white;
	position: absolute;
	top: 50%;
	left: 50%;
	-webkit-transform: translate(-50%,-20%);
	    -ms-transform: translate(-50%,-20%);
	        transform: translate(-50%,-20%);
}
.diZhi{
	height: 300rpx;
	background: #FFF1DB;
	width: 100%;
	position: relative;
}
.diZhi_shouhuo{
	position: absolute;
	top: 40rpx;
	left: 80rpx;
}
.diZhi_tel{
	position: absolute;
	right: 80rpx;
	top: 40rpx;
	font-size: 25rpx;
}
.dinWeiTu{
	width: 40rpx;
	height: 50rpx;
	position: absolute;
	left: 15rpx;
	top: 105rpx;
}
.diZhi_dizhi{
	width: 80%;
	position: absolute;
	left: 80rpx;
	top: 110rpx;
}
.youBian{
	width: 30rpx;
	height: 50rpx;
	position: absolute;
	right: 15rpx;
	top: 105rpx;
}
.beiZhu{
	position: absolute;
	color: #00A0E9;
	bottom: 50rpx;
	left: 80rpx;
}
.dinDan{
	width: 100%;
	height: auto;
	margin-top: 30rpx;
	font-size: 30rpx;
	background: #fff;
	margin-bottom: 168rpx;
}
.dianpuMing{
	height: 60rpx;
	width: 100%;
	position: relative;
	border-bottom: 1px solid #F1F1F1;
}
.yuan{
	border-radius: 100%;
	border: 1px solid #979797;
	width: 30rpx;
	height: 30rpx;
	position: absolute;
	top: 50%;
	-webkit-transform: translateY(-50%);
	    -ms-transform: translateY(-50%);
	        transform: translateY(-50%);
	left: 30rpx;
}
.mingzi{
	position: absolute;
	top: 50%;
	-webkit-transform: translateY(-50%);
	    -ms-transform: translateY(-50%);
	        transform: translateY(-50%);
	left: 30rpx;
	left: 80rpx;
}
.shopXinXi{
	height: 300rpx;
	width: 100%;
	border-bottom: 1px solid #F1F1F1;
	position: relative;
}
.shopXinXi_img{
	width: 250rpx;
	height: 250rpx;
	position: absolute;
	top: 50%;
	-webkit-transform: translateY(-50%);
	    -ms-transform: translateY(-50%);
	        transform: translateY(-50%); 
	left: 30rpx;
}
.shopXinXi_txt{
	position: absolute;
	top: 30rpx;
	left: 300rpx;
	width: 430rpx;
	overflow: hidden;
	-o-text-overflow:ellipsis;
	   text-overflow:ellipsis;
	white-space: nowrap;
}
.shopXinXi_fenLei{
	position: absolute;
	top: 50%;
	left: 300rpx;
	-webkit-transform: translateY(-50%);
	    -ms-transform: translateY(-50%);
	        transform: translateY(-50%);
	color: #737373;
}
.shopXinXi_fenLei>text{
	margin-right: 30rpx;
	color: #A5A5A5;
	font-size: 25rpx;
}
.jiage{
	position: absolute;
	bottom: 30rpx;
	left: 300rpx;
}
.jiage>text:nth-child(1){
	color: #E02020;
	margin-right: 50rpx;
	font-weight: 900;
}
.jiage>text:nth-child(2){
	color: #737373;
	font-size: 25rpx;
	text-decoration:line-through
}
.shuliang{
	position: absolute;
	right: 30rpx;
	bottom: 30rpx;
	color: #737373;
}
.yunfei{
	height: 60rpx;
	width: 100%;
	position: relative;
	border-bottom: 1px solid #F1F1F1;
}
.yunfei>text{
	position: absolute;
	top: 50%;
	-webkit-transform: translateY(-50%);
	    -ms-transform: translateY(-50%);
	        transform: translateY(-50%);
}
.yunfei>text:nth-child(1){
	left: 30rpx;
	color: #B7B7B7;
}
.yunfei>text:nth-child(2){
	right: 30rpx;
	color: #737373;
	font-size: 25rpx;
}
.youHui>text:nth-child(2){
	font-size: 25rpx;
	color: #E02020;
}
.heji{
	height: 80rpx;
	border-bottom: 1px solid #F1F1F1;
	padding-top: 30rpx;
	padding-right: 30rpx;
}
.heji>view:nth-child(1){
	color: #E02020;
}
.heji>view:nth-child(2){
	color: #626262;
}
.liuYan>textarea{
	height: 300rpx;
	padding: 30rpx;
}
.diBu{
	height: 120rpx;
	position: fixed;
	bottom: 0;
	width: 100%;
	background: #FF4343;
	z-index: 10;
}
.diBu>view{
	font-size:45rpx;
	/* font-weight: 900; */
	position: absolute;
	top: 50%;
	-webkit-transform: translateY(-50%);
	    -ms-transform: translateY(-50%);
	        transform: translateY(-50%);
	left: 30rpx;
	color: #fff;
}
.diBu>button{
	height: 80rpx;
	position: absolute;
	top: 50%;
	-webkit-transform: translateY(-50%);
	    -ms-transform: translateY(-50%);
	        transform: translateY(-50%);
	right: 30rpx;
	width: 250rpx;
	border-radius: 50rpx;
	color: #FF4343;
	background: #fff;
	line-height: 80rpx;
}

