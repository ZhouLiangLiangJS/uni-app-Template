
.tanChu{
	position: absolute;
	width: 100rpx;
	z-index: 8;
	height: 160rpx;
	padding: 10rpx;
	background-color: #626262;
	border-radius:10rpx;
	right: 20rpx;
	top: 75rpx;
}
.tanChu>.tanChu-jianTou{
	width: 20rpx;
	height: 20rpx;
	background-color: #626262;
	position: absolute;
	right: 25rpx;
	top: -8rpx;
	-webkit-transform: rotate(45deg);
	    -ms-transform: rotate(45deg);
	        transform: rotate(45deg);
	z-index: 8;
}
.tanChu>.tanChu-title{
	width: 100%;
	height: 25%;
	font-size: 25rpx;
	color: #fff;
	text-align: center;
	line-height: 40rpx;
	z-index: 9;
}
.hader-box{
	width: 100%;
	height: 168rpx;
	background: #F53D4A;
	position: relative;
}
.hader-box-back{
	width: 40rpx;
	height: 40rpx;
	margin-top: 75rpx;
	margin-left: 20rpx;
}
.hader-box-text{
	color: white;
	margin-left: 260rpx;
}
.main-box{
	margin: 40rpx auto;
	border-radius: 6px;
	width: 92%;
	background: #fff;
	-webkit-box-shadow: 0px 0px 5px #ccc;
	        box-shadow: 0px 0px 5px #ccc;
	padding: 20rpx;
	position: relative;
}
.main-box-title{
	width: 100%;
	height: 60rpx;
	border-bottom: 1px solid #bfbfbf;
}
.main-box-title>text{
	display: inline-block;
	width: 20%;
	overflow:hidden;
	white-space:nowrap;
	-o-text-overflow:ellipsis;
	   text-overflow:ellipsis;
}
.main-box-content{
	width: 100%;
	height: 130rpx;
}
.main-box-bot{
	width: 100%;
	height: 60rpx;
	line-height: 60rpx;
	border-top: 1px solid #bfbfbf;
}
.main-box-bottom{
	width: 100%;
	height: 80rpx;
	background-color: #F53D4A;
	border-radius:20rpx;
	text-align: center;
	line-height: 80rpx;
	color: #fff;
	font-size: 30rpx;
}
.main-box-title>text{
	font-size: 30rpx;
	letter-spacing: 1px;
}
.main-box-content>view>text{
	font-size: 30rpx;
	letter-spacing: 1px;
}
.main-box-bot>text{
	display: inline-block;
	width: 20%;
	overflow:hidden;
	white-space:nowrap;
	-o-text-overflow:ellipsis;
	   text-overflow:ellipsis;
	font-size: 30rpx;
	letter-spacing: 1px;
}
.main-box-content-gongZuo text{
	display: inline-block;
	width: 20%;
	overflow:hidden;
	white-space:nowrap;
	-o-text-overflow:ellipsis;
	   text-overflow:ellipsis;
}
.gray{
	color: #a4a4a4;
}
.main-box-top-img{
	width: 20rpx;
	position: absolute;
	right: 20rpx;
	top: 20rpx;
}

