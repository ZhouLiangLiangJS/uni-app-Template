
.muBu{
	width: 100%;
	height: 100%;
	background: #000000;
	opacity: .5;
	position: fixed;
	top: 0;
	z-index: 11;
}
.Xuanlist{
	width: 100%;
	height: 900rpx;
	position: fixed;
	z-index: 12;
	bottom: 100rpx;
	border-radius: 30rpx 30rpx 0 0;
	font-size: 30rpx;
	overflow: hidden;
	background: #fff;
}
.Xuanlist>view:nth-child(1){
	width: 100%;
	height: 20%;
	position: relative;
}
.Xuanlist>view:nth-child(2){
	width: 100%;
	height: 80%;
	position: relative;
	overflow-y: auto;
}
.Xuanlist>view:nth-child(1)>image{
	height: 95%;
	width: 27%;
	margin-left: 2%;
	margin-top: 2%;
}
.Xuanlist>view:nth-child(1)>.jg{
	position: absolute;
	left: 300rpx;
	top: 40rpx;
}
.Xuanlist>view:nth-child(1)>.fl{
	position: absolute;
	left: 300rpx;
	top: 80rpx;
}
.Xuanlist>view:nth-child(1)>.fi{
	position: absolute;
	left: 300rpx;
	top: 120rpx;
}
.Xuanlist>view:nth-child(2)>view{
	border-top:1px solid #ccc;
	margin-top: 30rpx;
	position: relative;
}
.Xuanlist>view:nth-child(2)>view>text{
	position: absolute;
	top: -20rpx;
	background: #fff;
	left: 50rpx;
}
.Xuanlist>view:nth-child(2)>view>button{
	width: auto;
	margin: 30rpx;
	padding: 0;
	height: auto;
	display: inline-block;
	line-height: 30rpx;
	font-size:30rpx;
	padding: 30rpx;
}
.nav-bottom{
	height: 100rpx;
	width: 100%;
	background: #fff;
	position: fixed;
	bottom: 0;
	z-index: 13;
	-webkit-box-shadow: 0 0 5px #aaa;
	        box-shadow: 0 0 5px #aaa;
}
.nav-bottom>view{
	display: inline-block;
	width: 40%;
	height: 70%;
	line-height: 70rpx;
	text-align: center;
	color: #fff;
	font-size: 30rpx;
	background: #F53D4A;
	border-radius:20rpx;
	position: absolute;
}
.nav-bottom>.gouWuChe{
	left: 30rpx;
	-webkit-transform: translateY(-50%);
	    -ms-transform: translateY(-50%);
	        transform: translateY(-50%);
	top: 50%;
}
.nav-bottom>.gouMai{
	right: 30rpx;
	-webkit-transform: translateY(-50%);
	    -ms-transform: translateY(-50%);
	        transform: translateY(-50%);
	top: 50%;
}
.hader-box{
	width: 100%;
	height: 168rpx;
	background: #F53D4A;
	position: fixed;
	z-index: 10;
	top: 0;
}
.hader-box-back{
	width: 40rpx;
	height: 40rpx;
	margin-top: 75rpx;
	margin-left: 20rpx;
}
.hader-box-text{
	color: white;
	margin-left: 240rpx;
}
#shell{
	width: 100%;height: 380rpx;
	margin-top:168rpx;
}
.lbmg{
	width: 100% !important;height: 100%;
}
.main-div{
	width: 100%;
	height: 250rpx;
	position: relative;
	border-bottom:1px solid #C9C9C9 ;
}
.main-div>view:nth-child(1){
	width: 100%;
	height: 150rpx;
	position: absolute;
	top: 50%;
	-webkit-transform: translateY(-70%);
	    -ms-transform: translateY(-70%);
	        transform: translateY(-70%);
}
.main-div>view:nth-child(1)>view{
	position: absolute;
	right: 20rpx;
	width: 100rpx;
	text-align: center;
	font-size: 30rpx;
	top: 0;
	color: #F53D4A;
}
.main-div>view:nth-child(1)>view>image{
	width: 100%;
}
.main-div>view:nth-child(1)>text:nth-child(1){
	position: absolute;
	top: 20rpx;
	width: 80%;
	left: 20rpx;
	font-size: 30rpx
}
.main-div>view:nth-child(1)>text:nth-child(2){
	position: absolute;
	bottom: 20rpx;
	left: 20rpx;
	font-size: 30rpx;
	color: #F53D4A;
}
.main-div>view:nth-child(2){
	font-size: 30rpx;
	position: absolute;
	bottom: 20rpx;
	left: 20rpx;
}
.main-div>view:nth-child(2)>text{
	margin-right: 40rpx;
	color: #808080;
}
.xinYong{
	width: 100%;
	border-bottom: 1px solid  #C9C9C9;
}
.xinYong>text{
	display: inline-block;
	width: 25%;
	height: 100rpx;
	text-align: center;
	line-height: 100rpx;
	font-size: 30rpx;
	color: #F53D4A;
}
.fenLei{
	width: 100%;
	padding: 10rpx 0 30rpx 0;
}
.fenLei>text{
	display: inline-block;
	width: 50%;
	text-align: center;
	font-size: 30rpx;
}
.fenLei_div>view:nth-child(1){
	width: 100%;
}
.fenLei_div>view:nth-child(1)>image{
	width: 100%;
}
.tuiJian{
	width: 92%;
	padding: 0 4%;
	margin-bottom: 100rpx;
	overflow: hidden;
}
.tuiJian_title{
	color: #000000;
	width: 100%;
	font-size: 30rpx;
	height: 60rpx;
}
.tuiJian_div{
	float: left;
	width: 47%;
	height: 480rpx;
	border-radius: 10rpx;
	-webkit-box-shadow: -1px 1px 5px #aaa;
	        box-shadow: -1px 1px 5px #aaa;
	overflow: hidden;
	margin-bottom: 40rpx;
}
.tuiJian_div:nth-child(even){
	margin-right: 6%;
}
.tuiJian_div .tuiJian_div_img{
	width: 100%;
	height: 66%;
}
.tuiJian_div .tuiJian_div_img>image{
	width: 100%;
	height: 100%;
}
.tuiJian_div_top{
	height: 14%;
	position: relative;
	padding: 0 10rpx;
}
.tuiJian_div_top text{
	overflow:hidden; 
	-o-text-overflow:ellipsis; 
	   text-overflow:ellipsis;
	display:-webkit-box; 
	-webkit-box-orient:vertical;
	-webkit-line-clamp:2; 
	position: absolute;
	font-size: 27rpx;
	color: #000;
}
.tuiJian_div_bottom{
	height: 20%;
	font-size: 30rpx;
	position: relative;
	padding: 0 10rpx;
}
.tuiJian_div_bottom text{
	position: absolute;
}
.tuiJian_div_bottom .tuiJian_div_bottom_jiaGe{
	color: #F53D4A;
	bottom: 5rpx;
}
.tuiJian_div_bottom .tuiJian_div_bottom_fuKuan{
	top: 10rpx;
	font-size: 23rpx;
	color: #808080;
}
.tuiJian_div_bottom image{
	position: absolute;
	bottom: 0;
	right: 10rpx;
	width:60rpx;
	height: 60rpx;
}

