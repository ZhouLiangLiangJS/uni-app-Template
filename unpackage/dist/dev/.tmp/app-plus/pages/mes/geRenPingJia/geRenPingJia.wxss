
.bg{
	width: 100%;
	position: fixed;
}
.hader-box{
	width: 100%;
	height: 168rpx;
	background: #F53D4A;
	position: fixed;
	top: 0;
	z-index: 10;
}
.hader-box>image{
	width: 40rpx;
	height: 40rpx;
	position: absolute;
	top: 60%;
	left: 20rpx;
	-webkit-transform: translateY(-50%);
	    -ms-transform: translateY(-50%);
	        transform: translateY(-50%);
}
.hader-box-text{
	color: white;
	position: absolute;
	top: 60%;
	left: 50%;
	-webkit-transform: translate(-50%,-50%);
	    -ms-transform: translate(-50%,-50%);
	        transform: translate(-50%,-50%);
}
.fenLei{
	position: fixed;
	top: 168rpx;
	width: 100%;
	height: 60rpx;
	-webkit-box-shadow: 0 0 5px #CCCCCC;
	        box-shadow: 0 0 5px #CCCCCC;
	z-index: 10;
	background-color: #fff;
}
.fenLei image{
	width: 30rpx;
	height: 40rpx;
	position: absolute;
	left: 80rpx;
	top: 50%;
	-webkit-transform: translateY(-50%);
	    -ms-transform: translateY(-50%);
	        transform: translateY(-50%);
}
.fenLei>view{
	width: 50%;
	display: inline-block;
	height: 100%;
	text-align: center;
	position: relative;
}
.fenLei>view>text{
	line-height: 60rpx;
	color: #A5A5A5;
	font-size: 30rpx;
}
.main-box{
	margin-top: 228rpx;
	padding: 30rpx 30rpx;
	font-size: 30rpx;
	min-height: 100vh;
}
.main-box-my{
	background-color: #fff;
	width: 94%;
	overflow: hidden;
	padding: 20rpx  3%;
	border-radius:20rpx;
}
.main-box-my-image{
	width: 25%;
	height: 150rpx;
	float: left;
	position: relative;
}
.main-box-my-image>image{
	width: 130rpx;
	height: 130rpx;
	position: absolute;
	left: 50%;
	top: 50%;
	-webkit-transform: translate(-50%,-50%);
	    -ms-transform: translate(-50%,-50%);
	        transform: translate(-50%,-50%);
}
.main-box-my-main{
	width: 75%;
	height: 150rpx;
	float: left;
}
.main-box-my-main>view{
	float: left;
}
.main-box-my-main>.main-box-my-main-top{
	width: 50%;
	height: 100rpx;
}
.main-box-my-main>.main-box-my-main-top>view>text{
	display: block;
	width: 100%;
	height: 100%;
	overflow: hidden;
	-o-text-overflow:ellipsis;
	   text-overflow:ellipsis;
	white-space: nowrap;
}
.main-box-my-main-top image{
	width: 30rpx;
	height: 30rpx;
}
.main-box-my-main-top>view{
	width: 100%;
	height: 50%;
}
.main-box-my-beiZhu{
	width: 100%;
	float: left;
	margin-top: 30rpx;
}
.main-box-my-tel{
	height: 60rpx;
	width: 100%;
	float: left;
	margin:20rpx 0;
}
.main-box-my-tel>image{
	width: 60rpx;
	height: 60rpx;
	float: right;
	margin-right: 40rpx;
}
.main-box-that{
	width: 94%;
	padding: 20rpx 3%;
	overflow: hidden;
	background-color: #fff;
	border-radius: 20rpx;
	margin-top: 30rpx;
}
.main-box-that-top{
	height: 100rpx;
	width: 100%;
}
.main-box-that-top-left,.main-box-that-top-right{
	height: 100%;
	float: left;
}
.main-box-that-top-left{
	width: 15%;
	position: relative;
}
.main-box-that-top-left>image{
	width: 60rpx;
	height: 60rpx;
	position: absolute;
	border-radius: 100%;
	left: 50%;
	-webkit-transform: translate(-50%,0);
	    -ms-transform: translate(-50%,0);
	        transform: translate(-50%,0);
}
.main-box-that-top-right{
	width: 85%;
}
.main-box-that-top-right>view{
	width: 100%;
	height: 50%;
}
.main-box-that-top-right>view>text{
	margin-right: 30rpx;
}
.main-box-that-top-right>view>image{
	width: 30rpx;
	height: 30rpx;
}
.main-box-that-bottom{
	width: 85%;
	margin-top: 20rpx;
	float: right;
	margin-bottom: 40rpx;
}

