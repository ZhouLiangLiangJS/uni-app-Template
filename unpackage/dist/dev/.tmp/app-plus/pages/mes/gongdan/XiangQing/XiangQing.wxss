
.hader-box{
	width: 100%;
	height: 168rpx;
	background: #F53D4A;
	position: fixed;
	top: 0;
}
.hader-box-back{
	width: 40rpx;
	height: 40rpx;
	margin-top: 75rpx;
	margin-left: 20rpx;
}
.hader-box-text{
	color: white;
	margin-left: 245rpx;
}
.main-box{
	margin-top:168rpx;
}
.nav-bottom{
	height: 100rpx;
	width: 100%;
	background-color: #F53D4AFF;
	position: fixed;
	font-size: 30rpx;
	bottom: 0;
}
.nav-bottom>view{
	height: 80%;
	width: 50%;
	float: left;
	font-size: 30rpx;
	text-align: center;
	line-height: 80rpx;
	color: #fff;
	position: absolute;
	top: 50%;
	-webkit-transform: translateY(-50%);
	    -ms-transform: translateY(-50%);
	        transform: translateY(-50%);
}
.nav-bottom>view:nth-child(1){
	left: 0;
}
.nav-bottom>view:nth-child(2){
	right: 0;
}
.main-box-top{
	width: 90%;
	background-color: #FFFFFF;
	margin-bottom: 20rpx;
	padding: 20rpx 5%;
}
.main-box-top-top{
	width: 100%;
	overflow: hidden;
}
.main-box-top-top>view{
	float: left;
	display: inline-block;
	width: 50%;
	font-size: 30rpx;
	margin-bottom: 20rpx;
}
.main-box-top-top>view>text:nth-child(1){
	color: #A6A6A6;
}
.main-box-top-top>.qiangDanShiJian{
	font-size: 25rpx;
	width: 100%;
}
.main-box-top-bottom{
	width: 100%;
	background-color: #FFFFFF;
	border-top:1px solid #BFBFBF;
}
.main-box-top-bottom>view{
	margin-top: 20rpx;
	font-size: 30rpx
}
.main-box-top-bottom>view>text:nth-child(1){
	color: #A6A6A6;
}
.main-box-top-bottom>view:nth-child(1)>text:nth-child(3){
	color: #A6A6A6;
	margin-left: 50rpx;
}
.main-box-main{
	width: 90%;
	background-color: #FFFFFF;
	margin-bottom: 20rpx;
	padding: 20rpx 5%;
}
.main-box-main>view{
	display: inline-block;
	font-size: 30rpx;
	width: 100%;
}
.main-box-main-top{
	border-bottom:1px solid #BFBFBF ;
}
.main-box-main>.main-box-main-top>view{
	margin-bottom:20rpx;
}
.main-box-main>.main-box-main-top>view:nth-child(1)>text:nth-child(1){
	color: #A6A6A6;
}
.main-box-main>.main-box-main-top>view:nth-child(1){
	width:100%;
}
.main-box-main>.main-box-main-top>view:nth-child(2){
	width:100%;
}
.main-box-main>.main-box-main-top>view:nth-child(2)>view{
	display: inline-block;
	width: 50%;
}
.main-box-main>.main-box-main-top>view>view>text:nth-child(odd){
	color: #A6A6A6;
}
.main-box-main-center{
	width: 100%;
	padding-bottom:20rpx;
	border-bottom:1px solid #BFBFBF;
}
.main-box-main-center>view{
	margin-top:20rpx;
	width: 100%;
}
.main-box-main-center>view>text:nth-child(1){
	color: #A6A6A6;
}
.main-box-main-bottom>view{
	width: 100%;
	margin-top: 20rpx;
}
.main-box-main-bottom>view>text:nth-child(1){
	color: #A6A6A6;
}
.main-box-main-bottom>view>text:nth-child(2){
	float: right;
}
.main-box-bottom{
	background-color: #fff;
	font-size: 30rpx;
	width: 90%;
	padding: 20rpx 5%;
}
.main-box-bottom>view{
	width: 100%;
	margin-bottom: 20rpx;
}
.main-box-bottom>view>text:nth-child(1){
	color: #A6A6A6;
}
.main-box-bottom>view>text:nth-child(2){
	float: right;
}

