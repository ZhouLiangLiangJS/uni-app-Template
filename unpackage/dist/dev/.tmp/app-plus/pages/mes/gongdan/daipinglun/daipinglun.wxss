
.hader-box{
	width: 100%;
	height: 168rpx;
	background: #F53D4A;
}
.hader-box-back{
	width: 40rpx;
	height: 40rpx;
	margin-top: 70rpx;
	margin-left: 20rpx;
}
.hader-box-text{
	color: white;
	margin-left: 260rpx;
}
.main-box{
	overflow: hidden;
	margin: 40rpx auto;
	border-radius: 6px;
	width: 92%;
	height: 380rpx;
	background: #fff;
	-webkit-box-shadow: 0px 0px 5px #CCCCCC;
	        box-shadow: 0px 0px 5px #CCCCCC;
	padding: 15rpx;
}
.main-box-title{
	width: 100%;
	height: 60rpx;
	border-bottom: 1px solid #bfbfbf;
	overflow:hidden; /*超出的部分隐藏起来。*/
	white-space:nowrap;/*不显示的地方用省略号...代替*/
	-o-text-overflow:ellipsis;
	   text-overflow:ellipsis;/* 支持 IE */
}
.main-box-title ._span{
	display: inline-block;
	width: 25rpx;
	height: 25rpx;
	border: 1px solid #7D7D7D;
	border-radius: 100%;
	margin-right:  30rpx;
}
.main-box-content{
	width: 100%;
	height: 160rpx;
}
.main-box-bot{
	width: 100%;
	height: 190rpx;
	border-top: 1px solid #bfbfbf;
}
.main-box-title>text{
	font-size: 30rpx;
	letter-spacing: 1px;
}
.main-box-content>view>text{
	display: inline-block;
	font-size: 30rpx;
	letter-spacing: 1px;
	overflow: hidden;
	white-space: nowrap;
	-o-text-overflow: ellipsis;
	   text-overflow: ellipsis;
}
.main-box-bot>view>view>text{
	font-size: 30rpx;
	letter-spacing: 1px;
}
.main-box-bot-float{
	float: left;
	margin-top: 26rpx;
}
.main-box-bot>view>text{
	font-size: 30rpx;
	letter-spacing: 1px;
}
.gray{
	color: #a4a4a4;
}
.btn-style{
	float: right;
	margin-right: 20rpx;
	color: #fff;
	background: #F53D4A;
	font-size: 30rpx;
	width: 84rpx;
	height: 44rpx;
	text-align: center;
	line-height: 44rpx;
	font-weight: 300;
	letter-spacing: 1px;
	-webkit-box-shadow: 0px 1px 3px #ccc;
	        box-shadow: 0px 1px 3px #ccc;
}

